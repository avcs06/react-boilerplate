{
  "name": "react-boilerplate",
  "version": "1.1.1-beta",
  "description": "A high performance react boilerplate",
  "author": "AvcS",
  "license": "UNLICENSED",
  "main": "server/index.js",
  "repository": "git@github.com:avcs06/react-boilerplate.git",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist/assets && cp -r assets/* dist/assets/",
    "server": "NODE_ENV=development nodemon server/index.js --verbose --watch",
    "watch": "npm run clean && NODE_ENV=development webpack --config webpack.config.js --display-modules --progress --profile --colors --watch",
    "build": "npm run clean && NODE_ENV=production webpack --config webpack.config.js",
    "setup": "node_modules/.bin/pm2 start server/bin/reactapp.js",
    "stop": "node_modules/.bin/pm2 stop server/bin/reactapp.js",
    "start": "node_modules/.bin/pm2 restart server/bin/reactapp.js"
  },
  "_moduleAliases": {
    "#root": ".",
    "#app": "app",
    "#server": "server",
    "#config": "config",
    "#common": "common",
    "#dist": "dist",
    "$lib": "app/lib",
    "$styles": "app/styles",
    "$actions": "app/actions",
    "$components": "app/components",
    "$containers": "app/containers",
    "@bin": "server/bin",
    "@lib": "server/lib",
    "@DAL": "server/DAL",
    "@handlers": "server/handlers"
  },
  "dependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cssnano": "^4.0.1",
    "express": "^4.16.3",
    "firstline": "^2.0.2",
    "ignore-styles": "^5.0.1",
    "immutable": "^4.0.0-rc.9",
    "isomorphic-fetch": "^2.2.1",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.0",
    "passerror": "^1.1.1",
    "pm2": "^2.10.3",
    "prop-types": "^15.5.10",
    "query-string": "^4.3.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "glob-all": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.1",
    "postcss-loader": "^2.1.6",
    "purgecss-webpack-plugin": "^1.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.18.2",
    "uglify-js": "^2.8.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^0.5.9",
    "webpack": "^4.16.0",
    "webpack-cli": "^2.1.5"
  },
  "keywords": []
}
