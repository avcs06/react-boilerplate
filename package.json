{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React+Redux Webpack Express+Mongo",
  "author": "AvcS",
  "license": "UNLICENSED",
  "main": "server/index.js",
  "repository": "git@gitlab.com:avcs06/RR-W-EM.git",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist/assets && cp -r assets/* dist/assets/",
    "server": "NODE_ENV=development nodemon server/index.js --watch",
    "watch": "npm run clean && NODE_ENV=development webpack --config webpack.config.js --progress --profile --colors --watch",
    "build": "npm run clean && NODE_ENV=production webpack --config webpack.config.js",
    "setup": "node_modules/.bin/pm2 start server/bin/avcspro.js",
    "stop": "node_modules/.bin/pm2 stop server/bin/avcspro.js",
    "start": "node_modules/.bin/pm2 restart server/bin/avcspro.js"
  },
  "dependencies": {
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
