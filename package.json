    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^4.11.0",
    "mongoose-relationship": "^0.1.5",
    "morgan": "^1.8.2",
    "passerror": "^1.1.1",
    "pm2": "^2.10.3",
    "prop-types": "^15.5.10",
    "query-string": "^4.3.4",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
